{
    "manifest_version": 3,
    "name": "Wordle Bot",
    "version": "0.1",
    "content_scripts": [
        {
        "matches":[
            "https://www.nytimes.com/games/wordle/index.html"
        ],
        "js": ["js/contentScript.js"]
        }
    ],
    "permissions": ["tabs"],
       "action": {
        "default_icon": "wordle.png",
        "default_popup": "popup.html",
        "default_title": "Wordle Hint"
    }

}
